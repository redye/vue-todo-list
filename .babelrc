{
  "presets": [
      "stage-1"
  ],
  "plugins": [
      "transform-vue-jsx",
      "syntax-dynamic-import"
  ],
  "env": {
    "browser": {
      "presets": [
        [
          "env", {
            "targets": {
              "browser": ["last 2 versions", "safari >= 7"]
            }
          }
        ]
      ]
    },
    "node": {
      "presets": [
        [
          "env", {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
